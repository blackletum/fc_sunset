"VertexLitGeneric"
{
	"$basetexture" "models\player\scientist\scientist_custom"
	"$bumpmap" "models\player\scientist\scientist_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" ".0"
	"$detailblendmode" "6"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1.5"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	"$blendtintbybasealpha"		"1"
	//"$blendtintcoloroverbase"		"1"
	"$colortint_base"		"{0 0 0}"
	"$color2"		"{93 72 117}"
	"$colortint_tmp"		"[0 0 0]"	

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"										// Boost for ambient cube component of rim lighting

	// Cloaking
	"$cloakPassEnabled" "1"

	"$envmap" "models/items/berserk_glow"
	"$envmaptint" "[0 0 0]"
	"$envmaptint_rage" "[1 0 0]"
	"$envmaptint_orig" "[0 0 0]"
	"$envmapfresnel" "1"	
	
	"$rage" "1"
	"$rage_compare" ".5"
	"$phongfresnelranges_rage"	"[0 1 10]"
	"$phongfresnelranges_orig"	"[.3 1 8]"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"ItemTintColor"
		{
			"resultVar"		"$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"		"$colortint_tmp"
			"srcVar2"		"$colortint_base"
			"resultVar"		"$color2"
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"RipAndTear"
		{
			"resultVar" "$rage"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$phongfresnelranges"
			"greaterVar"	"$phongfresnelranges_orig"
			"srcVar1"		"$rage"
			"srcVar2"		"$rage_compare"
			"resultVar"		"$phongfresnelranges"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$envmaptint"
			"greaterVar"	"$envmaptint_orig"
			"srcVar1"		"$rage"
			"srcVar2"		"$rage_compare"
			"resultVar"		"$envmaptint"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$phongfresnelranges"
			"greaterVar"	"$phongfresnelranges_rage"
			"srcVar1"		"$rage_compare"
			"srcVar2"		"$rage"
			"resultVar"		"$phongfresnelranges"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$envmaptint"
			"greaterVar"	"$envmaptint_rage"
			"srcVar1"		"$rage_compare"
			"srcVar2"		"$rage"
			"resultVar"		"$envmaptint"
		}
	}
}
